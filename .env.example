# Example environment configuration
# Copy this file to .env and adjust the values for your environment.
# Do NOT check real credentials into version control.

# Service ports
# HTTP_PORT=8080

# Shopware integration
SHOPWARE_ENABLED=false
SHOPWARE_BASE_URL=https://shopware.example.com
SHOPWARE_SALES_CHANNEL_ID=
SHOPWARE_CLIENT_ID=
SHOPWARE_CLIENT_SECRET=
# Optionally supply a pre-generated API token instead of client credentials.
SHOPWARE_ACCESS_TOKEN=
# Request timeout in milliseconds for outbound Shopware API calls.
SHOPWARE_REQUEST_TIMEOUT_MS=10000
